{
  "name": "sunnyw/composer-lib-starter",
  "description": "a template for writing composer library",
  "keywords": [
    "template",
    "composer",
    "lib",
    "library",
    "package",
    "sunnyw"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Wang Xi",
      "email": "iwisunny@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6"
  },
  "autoload": {
    "psr-4": {
      "sunnyw\\ComposerLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "prefer-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
